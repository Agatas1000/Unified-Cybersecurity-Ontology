                                                                                                          rdf:first "organization" ;
                                                                                                          rdf:rest rdf:nil
                                                                                                        ]
                                                                                             ]
                                                                                 ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf stx:Identity .


