                                                                                rdf:first "infrastructure-architect" ;
                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                           rdf:first "infrastructure-operator" ;
                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                      rdf:first "malware-author" ;
                                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                                 rdf:first "sponsor" ;
