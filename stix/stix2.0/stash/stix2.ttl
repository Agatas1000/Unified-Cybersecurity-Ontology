                                                                                 rdf:first " hacker" ;
                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                            rdf:first " insider-accidental" ;
                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                       rdf:first " insider-disgruntled" ;
