                                                                                                                             rdf:first " sensationalist" ;
                                                                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                                                                        rdf:first " spy" ;
                                                                                                                                        rdf:rest [ rdf:type rdf:List ;
