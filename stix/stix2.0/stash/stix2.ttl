@prefix : <http://purl.org/stix#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix stx: <http://purl.org/stix#> .
@prefix uco: <http://purl.org/cyber#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
#@base <http://purl.org/stix> .

<http://purl.org/stix> rdf:type owl:Ontology ;
                         owl:imports <http://ffrdc.ebiquity.umbc.edu/ns/ontology/> ;
                         rdfs:comment "An OWL ontology for representing cybersecurity information using the STIX 2 data model." .

#################################################################
#    Object Properties
#################################################################

###  http://purl.org/stix#attributed-to
stx:attributed-to rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf stx:stixObjectProperty ;
                  owl:inverseOf stx:attribution-of ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( stx:campaign
                                              stx:intrusion-set
                                              stx:threat-actor
                                            )
                              ] ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( stx:identity
                                             stx:intrusion-set
                                             stx:threat-actor
                                           )
                             ] .


###  http://purl.org/stix#attribution-of
stx:attribution-of rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf stx:stixObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( stx:identity
                                               stx:intrusion-set
                                               stx:threat-actor
                                             )
                               ] ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( stx:campaign
                                              stx:intrusion-set
                                              stx:threat-actor
                                            )
                              ] .


###  http://purl.org/stix#author-of
stx:author-of rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf stx:stixObjectProperty ;
              owl:inverseOf stx:authored-by .


###  http://purl.org/stix#authored-by
stx:authored-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf stx:stixObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( stx:malware
                                            stx:tool
                                          )
                            ] ;
                rdfs:range stx:threat-actor .


###  http://purl.org/stix#belongs-to
stx:belongs-to rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf stx:stixObjectProperty ;
               rdfs:domain stx:observable-objects ;
               rdfs:range stx:ASN .


###  http://purl.org/stix#commonObjectProperies
stx:commonObjectProperies rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf stx:stixObjectProperty ;
                          rdfs:domain stx:StixThing ;
                          rdfs:comment "Common properties and behavior across all STIX Domain Objects and STIX Relationship Objects" .


###  http://purl.org/stix#created_by_ref
stx:created_by_ref rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf stx:stixObjectProperty ;
                   owl:inverseOf stx:creator-of ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( stx:campaign
                                               stx:intrusion-set
                                               stx:threat-actor
                                             )
                               ] ;
                   rdfs:range stx:identity .


###  http://purl.org/stix#creator-of
stx:creator-of rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf stx:stixObjectProperty ;
               rdfs:domain stx:identity ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( stx:campaign
                                          stx:intrusion-set
                                          stx:threat-actor
                                        )
                          ] .


###  http://purl.org/stix#definition
stx:definition rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf stx:stixObjectProperty ;
               rdfs:domain stx:marking-definition ;
               rdfs:range stx:marking-object .


###  http://purl.org/stix#derivedFrom
stx:derivedFrom rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf stx:stixObjectProperty ;
                rdfs:domain stx:StixThing ;
                rdfs:range stx:StixThing .


###  http://purl.org/stix#duplicateOf
stx:duplicateOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf stx:stixObjectProperty ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain stx:StixThing ;
                rdfs:range stx:StixThing .


###  http://purl.org/stix#exploited-by
stx:exploited-by rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf stx:stixObjectProperty .


###  http://purl.org/stix#exploits
stx:exploits rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf stx:stixObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( stx:attack-pattern
                                         stx:campaign
                                         stx:malware
                                         stx:threat-actor
                                       )
                         ] ;
             rdfs:range stx:vulnerability .


###  http://purl.org/stix#external-reference
stx:external-reference rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf stx:commonObjectProperies ;
                       owl:inverseOf stx:external-reference-of ;
                       rdfs:domain stx:StixThing ;
                       rdfs:range stx:external-reference .


###  http://purl.org/stix#external-reference-of
stx:external-reference-of rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf stx:stixObjectProperty ;
                          owl:inverseOf stx:external_references ;
                          rdfs:domain stx:external-reference ;
                          rdfs:range stx:StixThing .


###  http://purl.org/stix#external_references
stx:external_references rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf stx:stixObjectProperty .


###  http://purl.org/stix#granular_marking
stx:granular_marking rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf stx:commonObjectProperies ;
                     rdfs:domain stx:StixThing ;
                     rdfs:range stx:granular-marking ;
                     rdfs:comment "a GranularMarking that apply to this object" .


###  http://purl.org/stix#hasHash
stx:hasHash rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf stx:stixObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( stx:external-reference
                                        stx:file
                                      )
                        ] ;
            rdfs:range stx:Hash .


###  http://purl.org/stix#hasPhase
stx:hasPhase rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf stx:stixObjectProperty ;
             owl:inverseOf stx:phaseOf ;
             rdfs:domain stx:kill-chain ;
             rdfs:range stx:kill-chain-phase .


###  http://purl.org/stix#has_sighting
stx:has_sighting rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf stx:stixObjectProperty ;
                 owl:inverseOf stx:sighting_of_ref .


###  http://purl.org/stix#impersonates
stx:impersonates rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf stx:stixObjectProperty ;
                 rdfs:domain stx:threat-actor ;
                 rdfs:range stx:identity ;
                 rdfs:comment "This Relationship describes that the Threat Actor impersonates the related Identity. For example, an  impersonates Relationship from the gh0st Threat Actor to the ACME Corp. Identity means that the actor known as gh0st impersonates ACME Corp." .


###  http://purl.org/stix#indicated_by
stx:indicated_by rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf stx:stixObjectProperty ;
                 owl:inverseOf stx:indicates .


###  http://purl.org/stix#indicates
stx:indicates rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf stx:stixObjectProperty ;
              rdfs:domain stx:indicator ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( stx:Adversary
                                         stx:TTP
                                       )
                         ] .


###  http://purl.org/stix#kill_chain_phases
stx:kill_chain_phases rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf stx:stixObjectProperty ;
                      rdfs:domain stx:malware ;
                      rdfs:range stx:kill-chain-phase .


###  http://purl.org/stix#mitigated_by
stx:mitigated_by rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf stx:stixObjectProperty ;
                 owl:inverseOf stx:mitigates ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( stx:TTP
                                             stx:vulnerability
                                           )
                             ] ;
                 rdfs:range stx:course-of-action .


###  http://purl.org/stix#mitigates
stx:mitigates rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf stx:stixObjectProperty ;
              rdfs:domain stx:course-of-action ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( stx:TTP
                                         stx:vulnerability
                                       )
                         ] .


###  http://purl.org/stix#object
stx:object rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf stx:stixObjectProperty ;
           rdfs:domain stx:StixThing ;
           rdfs:range stx:StixThing .


###  http://purl.org/stix#object_marking
stx:object_marking rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf stx:commonObjectProperies ;
                   rdfs:domain stx:StixThing ;
                   rdfs:range stx:marking-definition ;
                   rdfs:comment "a marking-definition object to be applied to this object" .


###  http://purl.org/stix#objects
stx:objects rdf:type owl:ObjectProperty ;
            rdfs:range stx:observable-objects .


###  http://purl.org/stix#opinionAbout
stx:opinionAbout rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf stx:stixObjectProperty ;
                 rdfs:domain stx:StixThing ;
                 rdfs:range stx:opinion .


###  http://purl.org/stix#phaseOf
stx:phaseOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf stx:stixObjectProperty ;
            rdfs:domain stx:kill-chain-phase ;
            rdfs:range stx:kill-chain .


###  http://purl.org/stix#relatedTo
stx:relatedTo rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf stx:stixObjectProperty ;
              rdf:type owl:TransitiveProperty ;
              rdfs:domain stx:StixThing ;
              rdfs:range stx:StixThing .


###  http://purl.org/stix#resolvesTo
stx:resolvesTo rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf stx:stixObjectProperty ;
               rdfs:domain stx:IpAddr ;
               rdfs:range stx:MacAddr .


###  http://purl.org/stix#sighting_of_ref
stx:sighting_of_ref rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf stx:stixObjectProperty ;
                    rdfs:domain stx:sighting ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( stx:Adversary
                                               stx:TTP
                                               stx:indicator
                                               stx:observable-objects
                                             )
                               ] .


###  http://purl.org/stix#source_of
stx:source_of rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf stx:stixObjectProperty .


###  http://purl.org/stix#source_ref
stx:source_ref rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf stx:stixObjectProperty ;
               rdfs:domain stx:relationship ;
               rdfs:range stx:SDO .


###  http://purl.org/stix#stixObjectProperty
stx:stixObjectProperty rdf:type owl:ObjectProperty .


###  http://purl.org/stix#target_of
stx:target_of rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf stx:stixObjectProperty ;
              owl:inverseOf stx:target_ref .


###  http://purl.org/stix#target_ref
stx:target_ref rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf stx:stixObjectProperty ;
               rdfs:domain stx:relationship ;
               rdfs:range stx:SDO .


###  http://purl.org/stix#targeted-by
stx:targeted-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf stx:stixObjectProperty ;
                owl:inverseOf stx:targets ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( stx:identity
                                            stx:vulnerability
                                          )
                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( stx:Adversary
                                           stx:TTP
                                         )
                           ] .


###  http://purl.org/stix#targets
stx:targets rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf stx:stixObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( stx:Adversary
                                        stx:TTP
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( stx:identity
                                       stx:vulnerability
                                     )
                       ] .


###  http://purl.org/stix#used-by
stx:used-by rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf stx:stixObjectProperty ;
            owl:inverseOf stx:uses ;
            rdfs:domain stx:TTP ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( stx:Adversary
                                       stx:TTP
                                     )
                       ] .


###  http://purl.org/stix#uses
stx:uses rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf stx:stixObjectProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( stx:Adversary
                                     stx:TTP
                                   )
                     ] ;
         rdfs:range stx:TTP .


###  http://purl.org/stix#variant-of
stx:variant-of rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf stx:stixObjectProperty ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain stx:malware ;
               rdfs:range stx:malware ;
               rdfs:comment "This variantOf relation is used to document that one piece of Malware is a variant of another piece of Malware. For example, TorrentLocker is a variant of CryptoLocker." .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/stix#attack-motivation
stx:attack-motivation rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf stx:property-label ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( stx:intrusion-set
                                                  stx:threat-actor
                                                )
                                  ] ;
                      rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:oneOf [ rdf:type rdf:List ;
                                               rdf:first "accidental" ;
                                               rdf:rest [ rdf:type rdf:List ;
                                                          rdf:first "coercion" ;
                                                          rdf:rest [ rdf:type rdf:List ;
                                                                     rdf:first "dominance" ;
                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                rdf:first "ideology" ;
                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                           rdf:first "notoriety" ;
                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                      rdf:first "organizational-gain" ;
                                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                                 rdf:first "personal-gain" ;
                                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                                            rdf:first "personal-satisfaction" ;
                                                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                                                       rdf:first "revenge" ;
                                                                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                  rdf:first "unpredictable" ;
                                                                                                                                                  rdf:rest rdf:nil
                                                                                                                                                ]
                                                                                                                                     ]
                                                                                                                          ]
                                                                                                               ]
                                                                                                    ]
                                                                                         ]
                                                                              ]
                                                                   ]
                                                        ]
                                             ]
                                 ] .


###  http://purl.org/stix#attack-resource-level
stx:attack-resource-level rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf stx:property-label ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( stx:intrusion-set
                                                      stx:threat-actor
                                                    )
                                      ] ;
                          rdfs:range [ rdf:type rdfs:Datatype ;
                                       owl:oneOf [ rdf:type rdf:List ;
                                                   rdf:first "club" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "contest" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "government" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "individual" ;
                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                               rdf:first "organization" ;
                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                          rdf:first "team" ;
                                                                                                          rdf:rest rdf:nil
                                                                                                        ]
                                                                                             ]
                                                                                  ]
                                                                       ]
                                                            ]
                                                 ]
                                     ] ;
                          rdfs:comment "Attack Resource Level is an open vocabulary that captures the general level of resources that a threat actor, intrusion set, or campaign might have access to. It ranges from individual, a person acting alone, to government, the resources of a national government." .


###  http://purl.org/stix#body
stx:body rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf stx:emailProperty ;
         rdfs:domain stx:email-message .


###  http://purl.org/stix#commonDataProperty
stx:commonDataProperty rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf stx:stixDataProperty ;
                       rdfs:domain stx:StixThing ;
                       rdfs:comment "a collection of data properties that can be used with any stix object" .


###  http://purl.org/stix#confidence
stx:confidence rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf stx:stixDataProperty ;
               rdfs:domain stx:opinion .


###  http://purl.org/stix#cpe
stx:cpe rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf stx:stixDataProperty ;
        rdfs:domain stx:Software .


###  http://purl.org/stix#created
stx:created rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf stx:commonDataProperty ;
            rdfs:domain stx:StixThing ;
            rdfs:comment "The created property represents the time at which the first version of this object was created. The timstamp value MUST be precise to the nearest millisecond." .


###  http://purl.org/stix#definition-type
stx:definition-type rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf stx:stixDataProperty ;
                    rdfs:domain stx:marking-definition .


###  http://purl.org/stix#description
stx:description rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf stx:stixDataProperty .


###  http://purl.org/stix#emailProperty
stx:emailProperty rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf stx:stixDataProperty ;
                  rdfs:domain stx:email-message .


###  http://purl.org/stix#hash-algorithm
stx:hash-algorithm rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf stx:property-label ;
                   rdfs:domain stx:StixThing ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:oneOf [ rdf:type rdf:List ;
                                            rdf:first "MD5" ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first "MD6" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "RIPEMD-160" ;
                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                             rdf:first "SHA-1" ;
                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                        rdf:first "SHA-224" ;
                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                   rdf:first "SHA-256" ;
                                                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                                                              rdf:first "SHA-384" ;
                                                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                                                         rdf:first "SHA-512" ;
                                                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                                                    rdf:first "SHA3-224" ;
                                                                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                                                                               rdf:first "SHA3-256" ;
                                                                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                          rdf:first "SHA3-384" ;
                                                                                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                     rdf:first "SHA3-512" ;
                                                                                                                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                                rdf:first "SSDEEP" ;
                                                                                                                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                                           rdf:first "WHIRLPOOL" ;
                                                                                                                                                                                           rdf:rest rdf:nil
                                                                                                                                                                                         ]
                                                                                                                                                                              ]
                                                                                                                                                                   ]
                                                                                                                                                        ]
                                                                                                                                             ]
                                                                                                                                  ]
                                                                                                                       ]
                                                                                                            ]
                                                                                                 ]
                                                                                      ]
                                                                           ]
                                                                ]
                                                     ]
                                          ]
                              ] .


###  http://purl.org/stix#hashType
stx:hashType rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf stx:stixDataProperty ;
             rdfs:domain stx:Hash .


###  http://purl.org/stix#id
stx:id rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf stx:commonDataProperty ;
       rdfs:domain stx:StixThing ;
       rdfs:range xsd:string ;
       rdfs:comment "The id property universally and uniquely identifies this object." .


###  http://purl.org/stix#identity-class
stx:identity-class rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf stx:property-label ;
                   rdfs:domain stx:identity ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:oneOf [ rdf:type rdf:List ;
                                            rdf:first "class" ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first "group" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "individual" ;
                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                             rdf:first "organization" ;
                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                        rdf:first "unknown" ;
                                                                                        rdf:rest rdf:nil
                                                                                      ]
                                                                           ]
                                                                ]
                                                     ]
                                          ]
                              ] .


###  http://purl.org/stix#indicator-label
stx:indicator-label rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf stx:property-label ;
                    rdfs:domain stx:indicator ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "anomalous-activity" ;
                                             rdf:rest [ rdf:type rdf:List ;
                                                        rdf:first "anonymization" ;
                                                        rdf:rest [ rdf:type rdf:List ;
                                                                   rdf:first "attribution" ;
                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                              rdf:first "benign" ;
                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                         rdf:first "compromised" ;
                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                    rdf:first "malicious-activity" ;
                                                                                                    rdf:rest rdf:nil
                                                                                                  ]
                                                                                       ]
                                                                            ]
                                                                 ]
                                                      ]
                                           ]
                               ] ;
                    rdfs:comment "Indicator labels is an open vocabulary used to categorize Indicators. It is intended to be high-level to promote consistent practices. Indicator labels should not be used to capture information that can be better captured via related Malware or Attack Pattern objects. It is better to link an Indicator to a Malware object describing Poison Ivy rather than simply labeling it with \"poison-ivy\"." .


###  http://purl.org/stix#industry-sector
stx:industry-sector rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf stx:property-label ;
                    rdfs:domain stx:identity ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "aerospace" ;
