###  https://purl.org/stix#attack-motivation
:attack-motivation rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :property-label ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :IntrusionSet
                                               :ThreatActor
                                             )
                               ] ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:oneOf [ rdf:type rdf:List ;
                                            rdf:first "accidental" ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first "coercion" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "dominance" ;
                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                             rdf:first "ideology" ;
                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                        rdf:first "notoriety" ;
                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                   rdf:first "organizational-gain" ;
                                                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                                                              rdf:first "personal-gain" ;
                                                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                                                         rdf:first "personal-satisfaction" ;
                                                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                                                    rdf:first "revenge" ;
                                                                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                                                                               rdf:first "unpredictable" ;
                                                                                                                                               rdf:rest rdf:nil
                                                                                                                                             ]
                                                                                                                                  ]
                                                                                                                       ]
                                                                                                            ]
                                                                                                 ]
                                                                                      ]
                                                                           ]
                                                                ]
                                                     ]
                                          ]
                              ] .


###  https://purl.org/stix#attack-resource-level
:attack-resource-level rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :property-label ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :IntrusionSet
                                                   :ThreatActor
                                                 )
                                   ] ;
                       rdfs:range [ rdf:type rdfs:Datatype ;
                                    owl:oneOf [ rdf:type rdf:List ;
                                                rdf:first "club" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "contest" ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "government" ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first "individual" ;
                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                            rdf:first "organization" ;
                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                       rdf:first "team" ;
                                                                                                       rdf:rest rdf:nil
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                  ] ;
                       rdfs:comment "Attack Resource Level is an open vocabulary that captures the general level of resources that a threat actor, intrusion set, or campaign might have access to. It ranges from individual, a person acting alone, to government, the resources of a national government." .


