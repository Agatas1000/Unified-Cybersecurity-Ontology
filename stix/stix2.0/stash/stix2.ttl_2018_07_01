                                                                                                           rdf:first "SHA-384" ;
                                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                                      rdf:first "SHA-512" ;
                                                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                                                 rdf:first "SHA3-224" ;
                                                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                                                            rdf:first "SHA3-256" ;
                                                                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                       rdf:first "SHA3-384" ;
                                                                                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                  rdf:first "SHA3-512" ;
