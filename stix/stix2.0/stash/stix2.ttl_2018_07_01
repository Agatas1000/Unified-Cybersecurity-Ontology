###  https://purl.org/stix#malware-label
:malware-label rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :property-label ;
               rdfs:domain :Malware ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "adware" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "backdoor" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "bot" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "ddos" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "dropper" ;
                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                               rdf:first "exploit-kit" ;
                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                          rdf:first "keylogger" ;
                                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                                     rdf:first "ransomware" ;
                                                                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                                                                rdf:first "remote-access-trojan" ;
                                                                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                                                                           rdf:first "resource-exploitation" ;
                                                                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                      rdf:first "rogue-security-software" ;
                                                                                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                 rdf:first "rootkit" ;
                                                                                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                            rdf:first "screen-capture" ;
                                                                                                                                                                            rdf:rest rdf:nil
                                                                                                                                                                          ]
                                                                                                                                                               ]
                                                                                                                                                    ]
                                                                                                                                         ]
                                                                                                                              ]
                                                                                                                   ]
                                                                                                        ]
                                                                                             ]
                                                                                  ]
                                                                       ]
                                                            ]
                                                 ]
                                      ]
                          ] ;
               rdfs:comment "Malware label is an open vocabulary that represents different types and functions of malware. Malware labels are not mutually exclusive; a malware instance can be both spyware and a screen capture tool." .


