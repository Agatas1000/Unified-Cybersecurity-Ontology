@prefix : <http://purl.org/stix#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix stx: <http://purl.org/stix#> .
@prefix uco: <https://purl.org/cyber#> .
@base <http://purl.org/stix> .

<http://purl.org/stix> rdf:type owl:Ontology ;
                        rdfs:comment "An OWL ontology for representing cybersecurity information using the STIX 2 data model." .

#################################################################
#    Object Properties
#################################################################

###  http://purl.org/stix#targeted-by
:targeted-by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <https://purl.org/stix#stixObjectProperty> ;
             owl:inverseOf <https://purl.org/stix#targets> ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( <https://purl.org/stix#Identity>
                                         <https://purl.org/stix#Vulnerability>
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( <https://purl.org/stix#Adversary>
                                        <https://purl.org/stix#TTP>
                                      )
                        ] .


